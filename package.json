{
    "name": "vue-sample-component",
    "version": "0.0.1",
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.38",
        "@babel/preset-env": "^7.0.0-beta.38",
        "@babel/preset-react": "^7.0.0-beta.38",
        "@vue/test-utils": "^1.0.0-beta.10",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^22.1.0",
        "babel-plugin-lodash": "^3.3.2",
        "babel-preset-es2015-rollup": "^3.0.0",
        "babel-preset-vue": "^2.0.0",
        "jest": "^22.1.4",
        "jest-serializer-vue": "^0.3.0",
        "jest-vue-preprocessor": "^1.3.1",
        "jsdom": "^11.6.1",
        "parse5": "^4.0.0",
        "rollup": "^0.55.1",
        "rollup-plugin-babel": "^4.0.0-beta.0",
        "rollup-plugin-commonjs": "^8.2.6",
        "rollup-plugin-conditional": "^1.1.1",
        "rollup-plugin-json": "^2.3.0",
        "rollup-plugin-livereload": "^0.6.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-root-import": "^0.2.2",
        "rollup-plugin-serve": "^0.4.2",
        "rollup-plugin-vue": "^3.0.0",
        "vue-jest": "^1.4.0",
        "vue-jest-utils": "^0.2.0",
        "vue-server-renderer": "^2.5.13",
        "vue-template-compiler": "^2.5.13"
    },
    "dependencies": {
        "lodash": "^4.17.4",
        "vue": "^2.5.13"
    },
    "scripts": {
        "build": "rollup -c --watch",
        "commit": "git add -A && git commit -m $npm_package_version;",
        "release-patch": "npm version patch --no-git-tag-version; npm run release;",
        "release-minor": "npm version minor --no-git-tag-version; npm run release;",
        "release-major": "npm version major --no-git-tag-version; npm run release;",
        "release": "npm run build-library; npm run uglify; npm run commit; npm publish;",
        "uglify": "npm run uglify-library;",
        "uglify-library": "./node_modules/.bin/uglifyjs dist/giveworks-form.js -o dist/giveworks-form.min.js;"
    },
    "jest": {
        "transform": {
            "^.+\\.js?$": "babel-jest",
            ".*\\.(vue)$": "jest-vue-preprocessor"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1",
            "^vue$": "vue/dist/vue.common.js"
        },
        "moduleDirectories": [
            "node_modules"
        ],
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ],
        "mapCoverage": true
    }
}
