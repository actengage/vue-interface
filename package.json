{
    "name": "vue-interface",
    "version": "0.3.6",
    "main": "./dist/vue-interface.js",
    "module": "./dist/vue-interface.es.js",
    "styles": "./dist/vue-interface.css",
    "scripts": {
        "dev": "npm run serve",
        "serve": "vue-cli-service build --target lib --name=VueInterface ./src/install.js --watch",
        "build": "vue-cli-service build --target lib --name=VueInterface ./src/install.js",
        "lint": "vue-cli-service lint",
        "release-patch": "npm version patch --no-git-tag-version; npm run release;",
        "release-minor": "npm version minor --no-git-tag-version; npm run release;",
        "release-major": "npm version major --no-git-tag-version; npm run release;",
        "release": "npm run build; npm run commit; npm publish; git push;",
        "fix": "./node_modules/.bin/eslint src --ext .js --ext .vue --fix"
    },
    "dependencies": {
        "bootstrap": "^4.1.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.12",
        "@fortawesome/free-regular-svg-icons": "^5.6.3",
        "@fortawesome/free-solid-svg-icons": "^5.6.3",
        "@fortawesome/vue-fontawesome": "^0.1.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.2",
        "@vue/cli-plugin-eslint": "^3.2.1",
        "@vue/cli-service": "^3.0.2",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-html": "^5.0.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^22.1.3",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.0.0-0",
        "html-beautify-webpack-plugin": "^1.0.5",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.22",
        "vue-template-compiler": "^2.5.17",
        "webpack-manifest-plugin": "^2.0.4"
    },
    "peerDependencies": {
        "axios": "^0.18.0",
        "moment": "^2.22.0",
        "popper.js": "^1.14.3",
        "vue": "^2.5.0"
    }
}
